{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "vault.json",
  "title": "Vault",
  "description": "A Yearn vault",
  "type": "object",
  "properties": {
    "comment": {
      "type": "string"
    },
    "hideAlways": {
      "type": "boolean"
    },
    "depositsDisabled": {
      "type": "boolean"
    },
    "withdrawalsDisabled": {
      "type": "boolean"
    },
    "apyOverride": {
      "type": "string"
    },
    "order": {
      "type": "number"
    },
    "migrationAvailable": {
      "type": "boolean"
    },
    "allowZapIn": {
      "type": "boolean"
    },
    "allowZapOut": {
      "type": "boolean"
    },
    "migrationContract": {
      "type": "string",
      "format": "address"
    },
    "migrationTargetVault": {
      "type": "string",
      "format": "address"
    },
    "retired": {
      "type": "boolean"
    },
    "displayName": {
      "type": "string"
    },
    "vaultIconOverride": {
      "type": "string"
    },
    "vaultSymbolOverride": {
      "type": "string"
    },
    "vaultNameOverride": {
      "type": "string"
    },
    "vaultDetailPageAssets": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": []
}
