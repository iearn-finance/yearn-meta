{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "vault.json",
  "title": "Vault",
  "description": "A Yearn vault",
  "type": "object",
  "properties": {
    "comment": {
      "type": "string"
    },
    "hideAlways": {
      "type": "boolean"
    },
    "hideIfNoDeposits": {
      "type": "boolean"
    },
    "depositsDisabled": {
      "type": "boolean"
    },
    "withdrawalsDisabled": {
      "type": "boolean"
    },
    "apyOverride": {
      "type": "string"
    },
    "order": {
      "type": "number"
    },
    "migrationAvailable": {
      "type": "boolean"
    },
    "latestVaultAddress": {
      "type": "string",
      "format": "address"
    }
  },
  "required": []
}
