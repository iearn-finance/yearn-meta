{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "vault.json",
  "title": "Vault",
  "description": "A Yearn vault",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "promoted": {
      "type": "boolean"
    },
    "retired": {
      "type": "boolean"
    },
    "depositDisabled": {
      "type": "boolean"
    },
    "depositDisabledMessage": {
      "type": "boolean"
    },
    "withdrawDisabled": {
      "type": "boolean"
    },
    "withdrawDisabledMessage": {
      "type": "boolean"
    },
    "apy": {
      "type": "object",
      "properties": {
        "calculation": {
          "type": "string",
          "default": ""
        }
      },
      "required": ["calculation"]
    }
  },
  "required": ["name", "description", "promoted", "apy"]
}
