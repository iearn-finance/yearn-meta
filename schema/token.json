{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "token.json",
  "title": "Token",
  "description": "An underlying token of a yearn vault",
  "type": "object",
  "properties": {
    "description": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "website": {
      "type": "string"
    }
  },
  "required": ["name"]
}
